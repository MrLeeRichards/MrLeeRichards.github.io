{
   "sentence": ["#sentenceBuild.capitalize#."],
   "sentenceBuild": [
      "#preface##clause#"
   ],
   "preface": [
      "although #clause#, ",
      "even though #clause#, ",
      "because #clause#, ",
      "in spite of the fact that #clause#, ",
      "especially since #clause#, ",
      "#clause#, but ",
      "#clause#. However, ",
      "#clause#. That being said, ",
      "#clause#. Even so, "
   ],
   "clause": [
      "#iWouldntSay##clauseStart# #always# #maybeAdverb##verb# #object##inManner#",
      "#clauseStart# #always# #maybeAdverb##verb# #object##inManner#"
   ],
   "clauseStart": [
      "#noun.a##adjPhrase# #will#",
      "every #noun##adjPhrase# #will#",
      "all #noun.s##adjPhrase# #will#",
      "any #noun.s##adjPhrase# #will#",
      "some #noun.s##adjPhrase# #will#",
      "most #noun.s##adjPhrase# #will#"
   ],
   "object": [
      "#noun.a#",
      "#noun.s#",
      "any #noun.s#",
      "all #noun.s#"
   ],
   "adjPhrase": [
      "",
      " of the #maybeAdverb##adjective# #type#",
      " of a #maybeAdverb##adjective# #type#"
   ],
   "inManner": [
      "",
      " in a #maybeAdverb##adjective# #manner#"
   ],
   "will": [
      "will",
      "must",
      "may",
      "might",
      "should",
      "can",
      "ought to"
   ],
   "orNot": [
      "",
      " not"
   ],
