      "manner",
      "fashion",
      "way"
   ],
   "type": [
      "sort",
      "type",
      "variety",
      "kind"
   ],
   "maybeAdverb": [
      "",
      "#adverb# "
   ],
   "iWouldntSay": [
      "",
      "I wouldn't #exactly##say# that ",
      "I wouldn't #exactly#go so far as to #say# that ",
      "I don't #exactly#think that ",
      "I don't #exactly#believe that ",
      "I #certainly#wouldn't #say# that ",
      "I #certainly#wouldn't go so far as to #say# that ",
      "I #certainly#don't think that ",
      "I #certainly#don't believe that "
   ],
   "iWouldSay": [
      "",
      "I #certainly#would #say# that ",
      "I #certainly#do think that ",
      "I #certainly#do believe that ",
      "I #certainly#do suspect that "
   ],
   "certainly": [
      "",
      "certainly ",
      "definitely ",
      "absolutely "
   ],
   "exactly": [
      "",
      "exactly ",
      "precisely ",
      "necessarily "
   ],
   "always": [
      "",
      "always ",
      "often ",
      "usually ",
      "occasionally ",
      "rarely ",
      "never "
   ],
   "say": [
      "say",
      "state",
      "assert",
      "insist",
      "suggest"
   ],
   "word": [
      "verb: #verb#",
      "noun: #noun#",
      "adjective: #adjective#",
      "adverb: #adverb#"
   ],
   "v": ["a", "e", "i", "o", "u"],
   "midStart": ["d","p","r","l","sh","s"],
   "midEnd": ["d","p","r","l","m","dd","pp","rr","ll","mm","dl"],
   "noun": ["#ate#", "#ite#", "#ation#", "#ization#", "#vdle#", "#vble#", "#vty#", "#ness#"],
   "verb": ["#ate#", "#ute#", "#ize#", "#vdle#", "#vble#"],
   "adjective": ["#ate#", "#ute#", "#ote#", "#ize#d", "#vdle#", "#vble#", "#al#"],
   "adverb": ["#ate#ly", "#ute#ly", "#ite#ly","#ote#ly", "#vdly#", "#vbly#", "#al#ly"],
   "root": [
      "#midStart##v##midEnd#"
   ],
   "ate": ["#root#ate", "#ation#ate"],
   "ation": ["#root#ation"],
   "ute": ["#root#ute"],
   "ite": ["#root#ite"],
   "ote": ["#root#ote"],
   "ize": ["#root#ize", "#ation#ize", "#al#ize"],
   "ization": ["#root#ization"],
   "al": ["#ation#al","#ization#al"],
   "vty": ["#root##v#ty","#root##v#tty"],
   "ness": ["#root##v#tiness","#root##v#ttiness","#root##v#ness"],
   "vdle": ["#root##v#dle","#root##v#ddle","#root##v#ndle"],
   "vble": ["#root##v#ble","#root##v#bble"],
   "vdly": ["#root##v#dly","#root##v#ddly","#root##v#ndly"],
   "vbly": ["#root##v#bly","#root##v#bbly"]
